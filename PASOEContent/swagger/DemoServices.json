openapi: "3.0.0"
info:
  version: 1.0.0
  title: Swagger Petstore
  license:
    name: MIT
servers:
  - url: http://localhost:8080/DemoRestServices/web/aerende
  - url: http://vdi5041:8080/DemoRestServices/web/aerende
  - url: http://V7240:8080/DemoRestServices/web/aerende
paths:
  /lista/{intrid}:
    get:
      summary: Lista p책 intressentens 채rende
      operationId: aerendeLista
      tags:
        - Aerende
      parameters:
        - name: intrid
          in: path
          description: Vems 채rende vill du se...
          required: true
          schema:
            type: integer
            format: int32
      responses:
        200:
          description: En lista med 채renden
          headers:
            x-next:
              description: A link to the next page of responses
              schema:
                type: string
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/Aerende"
        default:
          description: unexpected error
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/Error"

  /detalj/{aerendeid}:
    get:
      summary: Info for a specific case
      operationId: howCaseById
      tags:
        - Aerende
      parameters:
        - name: aerendeid
          in: path
          required: true
          description: The id of the case to retrieve
          schema:
            type: string
      responses:
        200:
          description: Expected response to a valid request
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/Pets"
        default:
          description: unexpected error
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/Error"
components:
  schemas:
    Claim:
      required:
        - Claimid
        - ClaimDate
        - ClaimStatusCode
        - ClaimStatusText
        - ClaimReportedCode
        - ClaimReportedText
        - ProductGroupCode
        - ProductGroupText
        - UserId
        - ConnectionId
        - CreatedDate
        - ClaimNumber
        - WarrantCode
        - InterperedClaimDate
        - ClaimStatusUpdateDate
        - AuthorizationUserId
        - ClaimDataChangedAfterAttestation
        - HealthWarrantCode
        - DocumentId
        - RecievedDate
        - MultipleEmplpoyerCode
      properties:
        Caimid:
          type: integer
          format: int64
        ClaimDate:
          type: string
          format: date
        ClaimStatusCode:
          type: string
        ClaimStatusText:
          type: string
        ClaimReportedCode:
          type: string
        ClaimReportedText:
          type: string
        ProductGroupCode:
          type: string
        ProductGroupText:
          type: string
        UserId:
          type: string
        ConnectionId:
          type: string
        CreatedDate:
          type: string
          format: date
        ClaimNumber:
          type: string
        WarrantCode:
          type: string
    Claims:
      type: array
      items:
        $ref: "#/components/schemas/Claim"
    Error:
      required:
        - code
        - message
      properties:
        code:
          type: integer
          format: int32
        message:
          type: string
